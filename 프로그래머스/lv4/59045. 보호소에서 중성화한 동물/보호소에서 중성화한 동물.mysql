SELECT AI.ANIMAL_ID, AI.ANIMAL_TYPE, AI.NAME
FROM ANIMAL_INS AI
LEFT OUTER JOIN ANIMAL_OUTS AO
ON AI.ANIMAL_ID = AO.ANIMAL_ID
WHERE (AI.SEX_UPON_INTAKE = 'Intact Female' AND AO.SEX_UPON_OUTCOME = 'Spayed Female')
OR (AI.SEX_UPON_INTAKE = 'Intact male' AND AO.SEX_UPON_OUTCOME = 'Neutered Male')
ORDER BY AI.ANIMAL_ID