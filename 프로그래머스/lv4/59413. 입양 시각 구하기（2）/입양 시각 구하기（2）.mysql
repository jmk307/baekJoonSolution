-- 코드를 입력하세요
WITH RECURSIVE A AS (
    SELECT 0 AS HOUR
    UNION ALL
    SELECT 1 + A.HOUR
    FROM A
    WHERE A.HOUR < 23 )

SELECT
A.HOUR, IFNULL(COUNT(HOUR(AO.DATETIME)), 0)
FROM A
LEFT OUTER JOIN ANIMAL_OUTS AO
ON A.HOUR = HOUR(AO.DATETIME)
GROUP BY A.HOUR

-- 가상 재귀테이블 만들고 LEFT JOIN하여 IFNULL이나 COALEASCE 사용하여 COUNT

